{
	"manifest_version": 2,
	"name": "Moodown",
	"version": "0.1.0",
	"description": "A batch downloader for moodle.nottingham.ac.uk",
	"icons": {
		"32": "icon32.png"
	},
	"page_action": {
		"default_icon": {
			"32": "icon32.png"
		},
		"default_title": "Moodown",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": [
			"background.js",
			"local-resource.js"
		]
	},
	"content_scripts": [{
		"matches": [
			"http://moodle.nottingham.ac.uk/course/view.php?id=*",
			"http://moodle.nottingham.ac.uk/mod/folder/view.php?id=*"
		],
		"js": [
			"content.js"
		]
	}],
	"permissions": [
		"tabs",
		"downloads",
		"*://moodle.nottingham.ac.uk/*",
		"https://js-agent.newrelic.com/nr-998.min.js",
		"webRequest",
		"webRequestBlocking"
	],
	"web_accessible_resources": [
		"res/*"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
